sudo mkdir /etc/luna
sudo chmod 777 -R /etc/luna

sudo cp -r . /etc/luna

sudo chmod +x /etc/luna/*
sudo chmod 777 -R /etc/luna/*

sudo ln -s /etc/luna/banner /usr/bin/banner
sudo ln -s /etc/luna/disco /usr/bin/disco
